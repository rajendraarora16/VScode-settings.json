{
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/*.js": {
            "when": "$(basename).ts"
        },
        "**/*.js.map": true,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "gitlens.keymap": "chorded",
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true
    },
    "telemetry.telemetryLevel": "off",
    "better-comments.multilineComments": true,
    "better-comments.highlightPlainText": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 200,
    "gitlens.views.fileHistory.enabled": true,
    "gitlens.views.lineHistory.enabled": true,
    "editor.minimap.enabled": true,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "terminal.integrated.fontFamily": "monospace",
    "git.autofetch": true,
    "git.fetchOnPull": true,
    "html.format.preserveNewLines": false,
    "html.format.wrapAttributes": "aligned-multiple",
    "html.format.wrapLineLength": 0,
    "html.format.indentInnerHtml": true,
    "terminal.integrated.automationShell.osx": "/bin/zsh",
    "workbench.colorTheme": "A Touch of Lilac Theme",
    "aws.telemetry": false,
    "javascript.format.insertSpaceAfterConstructor": true,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
    "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
    "javascript.format.semicolons": "insert",
    "js/ts.implicitProjectConfig.experimentalDecorators": true,
    "typescript.format.insertSpaceAfterConstructor": true,
    "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": false,
    "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": false,
    "typescript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": false,
    "typescript.format.insertSpaceAfterTypeAssertion": true,
    "typescript.format.insertSpaceBeforeFunctionParenthesis": false,
    "typescript.format.placeOpenBraceOnNewLineForControlBlocks": false,
    "typescript.format.placeOpenBraceOnNewLineForFunctions": false,
    "typescript.format.semicolons": "insert",
    "typescript.locale": "en",
    "typescript.suggest.completeFunctionCalls": true,
    "typescript.tsserver.watchOptions": {},
    "editor.fontFamily": "MesloLGS Nerd Font, Monaco, 'Courier New', monospace",
    "terminal.external.osxExec": "iTerm.app",
    "diffEditor.renderSideBySide": true,
    "aws.profile": "profile:treez-sandbox",
    "tslint.configFile": "tslint.config",
    "plantuml.render": "Local",
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false
}
